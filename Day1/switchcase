
package day1;

public class SwitchCase {
	public static void main(String[] args) {
		int a=10;
		int b=20;
		int cal=0;
		 switch(cal) {
		 case 1:
			 System.out.println("a+b");
			 break;
		 case 2:
			 System.out.println("a-b");
			 break;
		 case 3:
			 System.out.println("a*b");
			 break;
		 case 4:
			 System.out.println("a/b");
			 break;
			 		
			 		
		 
		 }
		}

}
