import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Enter the Starting value:");
         Scanner in=new Scanner(System.in);
         int a=in.nextInt();
         System.out.println("Enter the Ending value:");
         int b=in.nextInt();
         for (int i=a;i<b;i++){
             if(i%2==0){
                 System.out.println(i);
             }
         }
    
    }
}
