import java.util.ArrayList;
class ArrayListprogram{
    public static void main(String[] args) {
        ArrayList<Object> vn=new ArrayList<Object>();
        vn.add("A");
        vn.add("Computer Science");
        vn.add("3 years");
        vn.add(0,"Hari");
        vn.set(0,"john");
        
        for(Object  data:vn){
            System.out.println(data);
        
        }
    }
}
