public class MultiTh extends Thread{
    private String Name;
    MultiTh(String Name){
        this.Name=Name;
        
    }
    public void run(){
        for(int i=0;i<=5;i++){
            System.out.println(Name);
            try{
                Thread.sleep(500);
            }
            catch(InterruptedException e){
                e.printStackTrace();
                
                
            }
        }
    }
}
class Excecute{
    public static void main(String[] args){
        MultiTh m1=new MultiTh("Thread-1");
        MultiTh m2=new MultiTh("Thread-1");
        m1.start();
        m2.start();
    }
}
